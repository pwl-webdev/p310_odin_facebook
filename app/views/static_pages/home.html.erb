<div class="container">
	<% if user_signed_in? %>
		Welcome, <%= current_user.email %> !
		<%= link_to("Log out ", destroy_user_session_path, :method => :delete)%>
	<% else %>
		<%= render :template => "devise/sessions/new.html.erb" %>
		<div class="u-pull-right">
			<%= render :template => "devise/registrations/new.html.erb" %>
		</div>
	<% end %>
</div>